  <rdf:Description rdf:about="http://www.opengis.net/def/uom/SI/kelvin">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:seeAlso rdf:resource="http://www.bipm.org/en/si/si_brochure/chapter2/2-1/kelvin.html"/>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The kelvin, unit of thermodynamic temperature, is the fraction 1/273.16 of the thermodynamic temperature of the triple point of water.</skos:definition>
    <skos:exactMatch rdf:resource="http://qudt.org/vocab/unit#Kelvin"/>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">K</skos:notation>
  </rdf:Description>
