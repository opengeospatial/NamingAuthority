  <rdf:Description rdf:about="http://www.opengis.net/def/uom/SI/kelvin">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:seeAlso rdf:resource="http://www.bipm.org/en/si/si_brochure/chapter2/2-1/kelvin.html"/>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The kelvin, unit of thermodynamic temperature, is the fraction 1/273.16 of the thermodynamic temperature of the triple point of water.</skos:definition>
    <skos:exactMatch rdf:resource="http://qudt.org/vocab/unit#Kelvin"/>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">K</skos:notation>
    <skos:inScheme rdf:resource="http://www.opengis.net/def/uom"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.opengis.net/def/uom/SI/kilogram">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:seeAlso rdf:resource="http://www.bipm.org/en/si/si_brochure/chapter2/2-1/kilogram.html"/>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The kilogram is the unit of mass; it is equal to the mass of the international prototype of the kilogram.</skos:definition>
    <skos:exactMatch rdf:resource="http://qudt.org/vocab/unit#Kilogram"/>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">kg</skos:notation>
    <skos:inScheme rdf:resource="http://www.opengis.net/def/uom"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.opengis.net/def/uom/SI/second">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:seeAlso rdf:resource="http://www.bipm.org/en/si/si_brochure/chapter2/2-1/second.html"/>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The second is the duration of 9 192 631 770 periods of the radiation corresponding to the transition between the two hyperfine levels of the ground state of the caesium 133 atom.</skos:definition>
    <skos:exactMatch rdf:resource="http://qudt.org/vocab/unit#Second"/>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">s</skos:notation>
    <skos:inScheme rdf:resource="http://www.opengis.net/def/uom"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.opengis.net/def/uom/SI/candela">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:seeAlso rdf:resource="http://www.bipm.org/en/si/si_brochure/chapter2/2-1/candela.html"/>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The candela is the luminous intensity, in a given direction, of a source that emits monochromatic radiation of frequency 540 x 1012 hertz and that has a radiant intensity in that direction of 1/683 watt per steradian.</skos:definition>
    <skos:exactMatch rdf:resource="http://qudt.org/vocab/unit#Candela"/>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">cd</skos:notation>
    <skos:inScheme rdf:resource="http://www.opengis.net/def/uom"/>
  </rdf:Description>
