  <rdf:Description rdf:about="http://www.opengis.net/def/uom/SI/ampere">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:seeAlso rdf:resource="http://www.bipm.org/en/si/si_brochure/chapter2/2-1/ampere.html"/>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The ampere is that constant current which, if maintained in two straight parallel conductors of infinite length, of negligible circular cross-section, and placed 1 m apart in vacuum, would produce between these conductors a force equal to 2 x 10–7 newton per metre of length.</skos:definition>
    <skos:exactMatch rdf:resource="http://qudt.org/vocab/unit#Ampere"/>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A</skos:notation>
  </rdf:Description>
