  <rdf:Description rdf:about="http://www.opengis.net/def/uom/SI/mole">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:seeAlso rdf:resource="http://www.bipm.org/en/si/si_brochure/chapter2/2-1/mole.html"/>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">	1.	The mole is the amount of substance of a system which contains as many elementary entities as there are atoms in 0.012 kilogram of carbon 12.
2.	When the mole is used, the elementary entities must be specified and may be atoms, molecules, ions, electrons, other particles, or specified groups of such particles.</skos:definition>
    <skos:exactMatch rdf:resource="http://qudt.org/vocab/unit#Mole"/>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">mol</skos:notation>
    <skos:inScheme rdf:resource="http://www.opengis.net/def/uom"/>
  </rdf:Description>
