    <rdfs:label>GeoTIFF Tag Register</rdfs:label>
    <ns1:status rdf:resource="http://www.opengis.net/def/status/valid"/>
    <ns1:collectionView rdf:resource="http://www.opengis.net/def/geotiff-tag/"/>
    <skos:prefLabel>GeoTIFF Tag Register</skos:prefLabel>
    <dcterms:source>OGC Naming Authority on behalf of relevant OGC Working Group</dcterms:source>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.opengis.net/def/geotiff-tag/ogc/1.1/33550">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:label>ModelPixelScaleTag</rdfs:label>
    <ns1:status rdf:resource="http://www.opengis.net/def/status/valid"/>
    <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2021-02-16T12:27:08.804790</dcterms:created>
    <rdfs:seeAlso rdf:resource="http://docs.opengeospatial.org/is/19-008r4/19-008r4.html#_requirements_class_modelpixelscaletag"/>
    <skos:definition>The ModelPixelScaleTag may be used to specify the size of raster pixel spacing in the model space units when the raster space can be embedded in the model space coordinate reference system without rotation</skos:definition>
    <skos:notation rdf:datatype="http://www.opengis.net/def/encoding/ogc/geotiff">33550</skos:notation>
    <skos:prefLabel>ModelPixelScaleTag</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.opengis.net/def/geotiff-tag/ogc/1.1/33922">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:label>ModelTiepointTag</rdfs:label>
    <ns1:status rdf:resource="http://www.opengis.net/def/status/valid"/>
    <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2021-02-16T12:27:08.805167</dcterms:created>
    <rdfs:seeAlso rdf:resource="http://docs.opengeospatial.org/is/19-008r4/19-008r4.html#_requirements_class_modeltiepointtag"/>
    <skos:definition>The ModelTiepointTag may be used to help define the transformation between raster space and model space through a set of raster-to-model tiepoints and scaling parameters</skos:definition>
    <skos:notation rdf:datatype="http://www.opengis.net/def/encoding/ogc/geotiff">33922</skos:notation>
    <skos:prefLabel>ModelTiepointTag</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.opengis.net/def/geotiff-tag/ogc/1.1/34736">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:label>GeoDoubleParamsTag</rdfs:label>
    <ns1:status rdf:resource="http://www.opengis.net/def/status/valid"/>
    <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2021-02-16T12:27:08.806240</dcterms:created>
    <rdfs:seeAlso rdf:resource="http://docs.opengeospatial.org/is/19-008r4/19-008r4.html#_requirements_class_geodoubleparamstag"/>
    <skos:definition>The GeoDoubleParamsTag supports the storage of parameter values when the values are of type Double</skos:definition>
    <skos:notation rdf:datatype="http://www.opengis.net/def/encoding/ogc/geotiff">34736</skos:notation>
    <skos:prefLabel>GeoDoubleParamsTag</skos:prefLabel>
  </rdf:Description>
