    <rdfs:label>GeoKeyDirectoryTag</rdfs:label>
    <ns1:status rdf:resource="http://www.opengis.net/def/status/valid"/>
    <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2021-02-16T12:27:08.805882</dcterms:created>
    <rdfs:seeAlso rdf:resource="http://docs.opengeospatial.org/is/19-008r4/19-008r4.html#_requirements_class_geokeydirectorytag"/>
    <skos:definition>The Keys in GeoTIFF (also called 'GeoKeys') are all referenced from the GeoKeyDirectoryTag tag</skos:definition>
    <skos:notation rdf:datatype="http://www.opengis.net/def/encoding/ogc/geotiff">34735</skos:notation>
    <skos:prefLabel>GeoKeyDirectoryTag</skos:prefLabel>
