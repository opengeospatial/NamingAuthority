gm:GM_Ring a skos:Concept ;
    dc:creator "Alexandre Robin" ;
    dc:date "2010-12-02" ;
    policy:status status:valid ;
    rdfs:seeAlso <http://www.iso.org/iso/iso_catalogue/catalogue_tc/catalogue_detail.htm?csnumber=26012> ;
