    policy:doctype <http://www.opengis.net/def/doc-type/is> ;
    rdfs:seeAlso "https://portal.opengeospatial.org/files/?artifact_id=28506" ;
    skos:altLabel "07-068r4"@en,
        "Web Coverage Service (WCS) - Transaction operation extension"@en ;
    skos:definition "This extension of the WCS standard specifies an additional Transaction operation that may optionally be implemented by WCS servers. This Transaction operation allows clients to add, modify, and delete grid coverages that are available from a WCS server. The Transaction operation request references or includes the new or modified coverage data, including all needed coverage metadata. " ;
    skos:inScheme <http://www.opengis.net/def/docs> ;
    skos:notation "338"^^policy:cite_spec_id,
        "07-068r4"^^policy:doc_no ;
    skos:prefLabel "Web Coverage Service (WCS) - Transaction operation extension"@en .

