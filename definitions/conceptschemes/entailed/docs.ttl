    rdfs:seeAlso "https://portal.opengeospatial.org/files/?artifact_id=16080" ;
    skos:altLabel "06-054r1"@en,
        "Image Geopostioning Service"@en ;
    skos:definition "This document specifies the interface to an Image Geopositioning Service that adjusts the georeferencing coordinate transformations of multiple images. This adjustment is normally done using a photogrammetric triangulation process, although other methods could be used. Such triangulation adjusts the parameter values of the image georeferencing coordinate transformations using a least-squares fitting process to measured image positions with known error statistics." ;
    skos:inScheme <http://www.opengis.net/def/docs> ;
    skos:notation "198"^^policy:cite_spec_id,
        "06-054r1"^^policy:doc_no ;
    skos:prefLabel "OpenGIS Image Geopostioning Service"@en .

