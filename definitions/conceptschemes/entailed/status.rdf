  <rdf:Description rdf:about="http://www.opengis.net/def/status/valid">
    <rdf:type rdf:resource="http://www.opengis.net/def/metamodel/ogc-na/Status"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
