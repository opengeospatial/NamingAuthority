    skos:prefLabel "OGC API - Coverages"@en ;
    skos:topConceptOf <http://www.opengis.net/def/standards-baseline> .

<http://www.opengis.net/def/interface/ogcapi-environmental-data-retrieval> a skos:Concept ;
    ns2:created "2021-10-14T12:27:08.804790"^^xsd:dateTime ;
    ns1:status <http://www.opengis.net/def/status/valid> ;
    rdfs:seeAlso <https://ogcapi.ogc.org/edr> ;
    skos:definition "OGC API - Environmental Data Retrieval provides a family of lightweight interfaces to access Environmental Data resources. Each resource addressed by this standard maps to a defined query pattern." ;
    skos:inScheme <http://www.opengis.net/def/standards-baseline> ;
    skos:prefLabel "OGC API - Environmental Data Retrieval"@en ;
    skos:topConceptOf <http://www.opengis.net/def/standards-baseline> .

<http://www.opengis.net/def/interface/ogcapi-features> a skos:Concept ;
    ns2:created "2020-02-16T12:27:08.804790"^^xsd:dateTime ;
    ns1:status <http://www.opengis.net/def/status/valid> ;
    rdfs:seeAlso <https://www.ogc.org/standards/ogcapi-features> ;
    skos:definition "OGC API - Features provides API building blocks to create, modify and query features on the Web. OGC API Features is comprised of multiple parts, each of them is a separate standard. This part, the 'Core', specifies the core capabilities and is restricted to fetching features where geometries are represented in the coordinate reference system WGS 84 with axis order longitude/latitude. " ;
    skos:inScheme <http://www.opengis.net/def/standards-baseline> ;
    skos:prefLabel "OGC API - Features"@en ;
    skos:topConceptOf <http://www.opengis.net/def/standards-baseline> .

<http://www.opengis.net/def/interface/ogcapi-maps> a skos:Concept ;
    ns2:created "2021-10-14T12:27:08.804790"^^xsd:dateTime ;
    ns1:status <http://www.opengis.net/def/status/submitted> ;
    rdfs:seeAlso <https://ogcapi.ogc.org/maps> ;
    skos:definition "OGC API - Maps describes an API that can serve spatially referenced and dynamically rendered electronic maps." ;
    skos:inScheme <http://www.opengis.net/def/standards-baseline> ;
    skos:prefLabel "OGC API - Maps"@en ;
    skos:topConceptOf <http://www.opengis.net/def/standards-baseline> .

<http://www.opengis.net/def/interface/ogcapi-processes> a skos:Concept ;
