    skos:prefLabel "OGC API - 3D Geovolumes"@en ;
    skos:topConceptOf <http://www.opengis.net/def/standards-baseline> .

<http://www.opengis.net/def/interface/ogcapi-common> a skos:Concept ;
    ns2:created "2021-10-14T12:27:08.804790"^^xsd:dateTime ;
    ns1:status <http://www.opengis.net/def/status/submitted> ;
    rdfs:seeAlso <https://ogcapi.ogc.org/common> ;
    skos:definition "The purpose of the OGC API - Common Standard is to define those fundamental building blocks and requirements which are applicable to all OGC Web API Standards." ;
    skos:inScheme <http://www.opengis.net/def/standards-baseline> ;
    skos:prefLabel "OGC API - Common"@en ;
    skos:topConceptOf <http://www.opengis.net/def/standards-baseline> .

<http://www.opengis.net/def/interface/ogcapi-coverages> a skos:Concept ;
    ns2:created "2021-10-14T12:27:08.804790"^^xsd:dateTime ;
    ns1:status <http://www.opengis.net/def/status/submitted> ;
    rdfs:seeAlso <https://ogcapi.ogc.org/coverages> ;
    skos:definition "OGC API - Coverages establishes how to access coverages as defined by the Coverage Implementation Schema (CIS) through a Web API such as those described by the OpenAPI specification." ;
