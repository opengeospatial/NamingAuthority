    <rdfs:label>OGC API</rdfs:label>
    <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2023-10-13T16:54:05.347625</dcterms:created>
    <rdfs:seeAlso rdf:resource="https://docs.ogc.org/is/19-072/19-072.html"/>
    <skos:altLabel>OGC Web API</skos:altLabel>
    <skos:definition>a Web API that implements one or more Conformance Classes from an OGC API Standard.</skos:definition>
    <skos:inScheme rdf:resource="http://www.opengis.net/def/glossary"/>
    <skos:prefLabel>OGC API</skos:prefLabel>
    <policy:status rdf:resource="http://www.opengis.net/def/status/valid"/>
    <skos:topConceptOf rdf:resource="http://www.opengis.net/def/glossary"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.opengis.net/def/glossary/term/ObjectOrientedOO">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:label>Object Oriented (OO)</rdfs:label>
    <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-13</dcterms:created>
    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-04-16</dcterms:modified>
    <policy:status rdf:resource="http://www.opengis.net/def/status/valid"/>
    <skos:definition>Software in which data and processing functions are packaged into small, discrete, interoperable modules, offering advantages such as portability and easy maintainability.</skos:definition>
    <skos:inScheme rdf:resource="http://www.opengis.net/def/glossary"/>
    <skos:prefLabel>Object Oriented (OO)</skos:prefLabel>
    <skos:topConceptOf rdf:resource="http://www.opengis.net/def/glossary"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.opengis.net/def/glossary/term/EUROGI">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:label>EUROGI</rdfs:label>
    <dc:source>PreANVIL Glossary</dc:source>
    <dc:source>European Umbrella Organisation for Geographic Information: members are national associations (e.g. The Association for Geographic Information in the UK) and related pan-European sector bodies.</dc:source>
    <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-13</dcterms:created>
    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-04-16</dcterms:modified>
    <policy:status rdf:resource="http://www.opengis.net/def/status/valid"/>
    <skos:inScheme rdf:resource="http://www.opengis.net/def/glossary"/>
    <skos:prefLabel>EUROGI</skos:prefLabel>
    <skos:topConceptOf rdf:resource="http://www.opengis.net/def/glossary"/>
    <skos:definition>EUROGI</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.opengis.net/def/glossary/term/Edge">
