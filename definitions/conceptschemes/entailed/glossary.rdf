    <skos:prefLabel>IPR</skos:prefLabel>
    <skos:topConceptOf rdf:resource="http://www.opengis.net/def/glossary"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.opengis.net/def/glossary/term/MMI">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:label>MMI</rdfs:label>
    <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-13</dcterms:created>
    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-04-16</dcterms:modified>
    <policy:status rdf:resource="http://www.opengis.net/def/status/valid"/>
    <skos:definition>OGC`s Multi-Hazard Mapping Initiative (MMI) Phase I (2001) was a pilot project sponsored by the US Federal Emergency Management Agency (FEMA) that established a limited operational framework of interoperable services to illustrate the advantages of using products with OGC interfaces to access, fuse and visualize critical spatial information in support of FEMA multi-hazard mitigation, response and recovery functions.</skos:definition>
    <skos:inScheme rdf:resource="http://www.opengis.net/def/glossary"/>
    <skos:prefLabel>MMI</skos:prefLabel>
    <skos:topConceptOf rdf:resource="http://www.opengis.net/def/glossary"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.opengis.net/def/glossary/term/ClassificationScheme">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
