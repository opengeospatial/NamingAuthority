    <skos:prefLabel>Long-Term Technical Baseline Target</skos:prefLabel>
    <skos:topConceptOf rdf:resource="http://www.opengis.net/def/glossary"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.opengis.net/def/glossary/term/Symbology">
