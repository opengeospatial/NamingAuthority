  <rdf:Description rdf:about="http://www.opengis.net/def/glossary/term/GeospatialData">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:label>geospatial data</rdfs:label>
    <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-13</dcterms:created>
    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-04-16</dcterms:modified>
    <policy:status rdf:resource="http://www.opengis.net/def/status/valid"/>
    <skos:definition>Location properties related to any terrestrial feature or phenomena. Location properties may include any information about the location or area of, and relationships among, and descriptive information about geographic features and phenomena. This includes remotely sensed data,  vector map data, addresses, coordinates, etc. Note that "geospatial data" is more precise in many contexts than "geographic data," because geospatial data is often used in ways that do not involve a graphic representation, or map, created from the data.</skos:definition>
    <skos:inScheme rdf:resource="http://www.opengis.net/def/glossary"/>
    <skos:prefLabel>geospatial data</skos:prefLabel>
    <skos:topConceptOf rdf:resource="http://www.opengis.net/def/glossary"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.opengis.net/def/glossary/term/Property">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:label>property</rdfs:label>
    <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-13</dcterms:created>
    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-04-16</dcterms:modified>
    <policy:status rdf:resource="http://www.opengis.net/def/status/valid"/>
    <skos:definition>A facet or attribute or an object referenced by a name.</skos:definition>
    <skos:inScheme rdf:resource="http://www.opengis.net/def/glossary"/>
    <skos:prefLabel>property</skos:prefLabel>
    <skos:topConceptOf rdf:resource="http://www.opengis.net/def/glossary"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.opengis.net/def/glossary/term/BusinessProcess">
