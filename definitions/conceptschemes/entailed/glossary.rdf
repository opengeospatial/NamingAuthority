    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-04-16</dcterms:modified>
    <policy:status rdf:resource="http://www.opengis.net/def/status/valid"/>
    <skos:definition>The definition of a specific observation type in accordance with the general observation model.</skos:definition>
    <skos:inScheme rdf:resource="http://www.opengis.net/def/glossary"/>
    <skos:prefLabel>observation domain model</skos:prefLabel>
    <skos:topConceptOf rdf:resource="http://www.opengis.net/def/glossary"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.opengis.net/def/glossary/term/W3C">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:label>W3C</rdfs:label>
    <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-13</dcterms:created>
    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-04-16</dcterms:modified>
    <policy:status rdf:resource="http://www.opengis.net/def/status/valid"/>
    <skos:definition>World Wide Web Consortium. The organization that manages standards for the Worldwide Web.</skos:definition>
    <skos:inScheme rdf:resource="http://www.opengis.net/def/glossary"/>
    <skos:prefLabel>W3C</skos:prefLabel>
