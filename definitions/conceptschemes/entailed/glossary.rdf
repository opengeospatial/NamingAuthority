  <rdf:Description rdf:about="http://www.opengis.net/def/glossary/term/DCAT">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:label>Data Catalog Vocabulary</rdfs:label>
    <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2023-10-13T16:54:05.346122</dcterms:created>
