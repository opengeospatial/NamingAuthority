    <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-13</dcterms:created>
    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-04-16</dcterms:modified>
    <policy:status rdf:resource="http://www.opengis.net/def/status/valid"/>
    <skos:definition>The ability of a user to move from one system to another without having to relearn everything necessary to use such as system.</skos:definition>
    <skos:inScheme rdf:resource="http://www.opengis.net/def/glossary"/>
    <skos:prefLabel>user portability</skos:prefLabel>
    <skos:topConceptOf rdf:resource="http://www.opengis.net/def/glossary"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.opengis.net/def/glossary/term/JPGJPEG">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:label>JPG (JPEG)</rdfs:label>
    <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-13</dcterms:created>
    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-04-16</dcterms:modified>
    <policy:status rdf:resource="http://www.opengis.net/def/status/valid"/>
    <skos:definition>JPEG (Joint Photographic Experts Group) Image format for continuous tone pictures: JPEG makes use of continuous-tone digital images much more economical by drastically reducing the volume required for storage and the bandwidth required for transmission.</skos:definition>
    <skos:inScheme rdf:resource="http://www.opengis.net/def/glossary"/>
    <skos:prefLabel>JPG (JPEG)</skos:prefLabel>
    <skos:topConceptOf rdf:resource="http://www.opengis.net/def/glossary"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.opengis.net/def/glossary/term/IPR">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:label>IPR</rdfs:label>
    <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-13</dcterms:created>
    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-04-16</dcterms:modified>
    <policy:status rdf:resource="http://www.opengis.net/def/status/valid"/>
