  <rdf:Description rdf:about="http://www.opengis.net/def/auth/auto2">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <policy:scope rdf:resource="http://www.opengis.net/def/crs/"/>
    <policy:status rdf:resource="http://www.opengis.net/def/status/valid"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">AUTO2 namespace for automatic projections</rdfs:label>
    <rdfs:seeAlso rdf:resource="https://portal.ogc.org/files/?artifact_id=14416"/>
    <skos:topConceptOf rdf:resource="http://www.opengis.net/def/auth"/>
    <skos:notation rdf:datatype="http://www.opengis.net/def/metamodel/ogc-na/lcname">auto2</skos:notation>
    <skos:prefLabel rdf:datatype="http://www.w3.org/2001/XMLSchema#string">AUTO2 namespace</skos:prefLabel>
    <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The 'AUTO2' namespace is used for 'automatic' coordinate reference systems; that is, for a class of CRSs that include a user-selected centre of projection. A complete 'AUTO2' CRS label comprises the 'AUTO2' namespace prefix, a numeric CRS identifier from the AUTO2 namespace, a number indicating what conversion factor to apply to convert between bounding box units and AUTO2 CRS units, and values for the central longitude and latitude in degrees: AUTO2:auto_crs_id,factor,lon0,lat0</skos:definition>
  </rdf:Description>
