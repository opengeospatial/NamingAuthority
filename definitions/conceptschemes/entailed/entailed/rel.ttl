@prefix ns1: <http://www.w3.org/2004/02/skos/core#> .
@prefix ns2: <http://www.opengis.net/def/metamodel/ogc-na/> .
@prefix ns3: <http://purl.org/dc/terms/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.opengis.net/def/register/> ns1:member <http://www.opengis.net/def/rel/> .

<http://www.opengis.net/def/rel> a ns1:ConceptScheme ;
    rdfs:label "OGC Link Relation Type Register" ;
    ns3:source "OGC Naming Authority on behalf of relevant OGC Working Group" ;
    ns2:collectionView <http://www.opengis.net/def/rel/> ;
    ns1:definition "This is a register of link relation types used by OGC standards. "@en ;
    ns1:prefLabel "OGC Link Relation Type Register"@en .

<http://www.opengis.net/def/rel/iana/1.0/alternate> a ns1:Concept ;
    rdfs:label "alternate" ;
    ns3:created "2020-06-10"^^xsd:date ;
    ns2:status <http://www.opengis.net/def/status/valid> ;
    rdfs:comment "Registered with IANA" ;
    ns1:definition " Refers to a substitute for this context."@en ;
    ns1:notation "alternate"^^<https://www.iana.org/assignments/link-relations> ;
    ns1:prefLabel "alternate"@en .

<http://www.opengis.net/def/rel/iana/1.0/collection> a ns1:Concept ;
    rdfs:label "collection" ;
    ns3:created "2020-06-10"^^xsd:date ;
    ns2:status <http://www.opengis.net/def/status/valid> ;
    rdfs:comment "Registered with IANA" ;
    ns1:definition " The target IRI points to a resource which represents the collection resource for the context IRI."@en ;
    ns1:notation "collection"^^<https://www.iana.org/assignments/link-relations> ;
    ns1:prefLabel "collection"@en .

<http://www.opengis.net/def/rel/iana/1.0/describedby> a ns1:Concept ;
    rdfs:label "describedby" ;
    ns3:created "2020-06-10"^^xsd:date ;
    ns2:status <http://www.opengis.net/def/status/valid> ;
    rdfs:comment "Registered with IANA" ;
    ns1:definition " Refers to a resource providing information about the link’s context."@en ;
    ns1:notation "describedby"^^<https://www.iana.org/assignments/link-relations> ;
    ns1:prefLabel "describedby"@en .

<http://www.opengis.net/def/rel/iana/1.0/item> a ns1:Concept ;
    rdfs:label "item" ;
    ns3:created "2020-06-10"^^xsd:date ;
    ns2:status <http://www.opengis.net/def/status/valid> ;
    rdfs:comment "Registered with IANA" ;
    ns1:definition " The target IRI points to a resource that is a member of the collection represented by the context IRI."@en ;
    ns1:notation "item"^^<https://www.iana.org/assignments/link-relations> ;
    ns1:prefLabel "item"@en .

<http://www.opengis.net/def/rel/iana/1.0/license> a ns1:Concept ;
    rdfs:label "license" ;
    ns3:created "2020-06-10"^^xsd:date ;
    ns2:status <http://www.opengis.net/def/status/valid> ;
    rdfs:comment "Registered with IANA" ;
    ns1:definition " Refers to a license associated with this context."@en ;
    ns1:notation "license"^^<https://www.iana.org/assignments/link-relations> ;
    ns1:prefLabel "license"@en .

<http://www.opengis.net/def/rel/iana/1.0/next> a ns1:Concept ;
    rdfs:label "next" ;
    ns3:created "2020-06-10"^^xsd:date ;
    ns2:status <http://www.opengis.net/def/status/valid> ;
    rdfs:comment "Registered with IANA" ;
    ns1:definition " Indicates that the link’s context is a part of a series, and that the next in the series is the link target."@en ;
    ns1:notation "next"^^<https://www.iana.org/assignments/link-relations> ;
    ns1:prefLabel "next"@en .

<http://www.opengis.net/def/rel/iana/1.0/prev> a ns1:Concept ;
    rdfs:label "prev" ;
    ns3:created "2020-06-10"^^xsd:date ;
    ns2:status <http://www.opengis.net/def/status/valid> ;
    rdfs:comment "Registered with IANA" ;
    ns1:definition " Indicates that the link’s context is a part of a series, and that the previous in the series is the link target. This relation is only used in examples."@en ;
    ns1:notation "prev"^^<https://www.iana.org/assignments/link-relations> ;
    ns1:prefLabel "prev"@en .

<http://www.opengis.net/def/rel/iana/1.0/self> a ns1:Concept ;
    rdfs:label "self" ;
    ns3:created "2020-06-10"^^xsd:date ;
    ns2:status <http://www.opengis.net/def/status/valid> ;
    rdfs:comment "Registered with IANA" ;
    ns1:definition " Conveys an identifier for the link’s context."@en ;
    ns1:notation "self"^^<https://www.iana.org/assignments/link-relations> ;
    ns1:prefLabel "self"@en .

<http://www.opengis.net/def/rel/iana/1.0/service-desc> a ns1:Concept ;
    rdfs:label "service-desc" ;
    ns3:created "2020-06-10"^^xsd:date ;
    ns2:status <http://www.opengis.net/def/status/valid> ;
    rdfs:comment "Registered with IANA" ;
    ns1:definition " Identifies service description for the context that is primarily intended for consumption by machines. API definitions are considered service descriptions."@en ;
    ns1:notation "service-desc"^^<https://www.iana.org/assignments/link-relations> ;
    ns1:prefLabel "service-desc"@en .

<http://www.opengis.net/def/rel/iana/1.0/service-doc> a ns1:Concept ;
    rdfs:label "service-doc" ;
    ns3:created "2020-06-10"^^xsd:date ;
    ns2:status <http://www.opengis.net/def/status/valid> ;
    rdfs:comment "Registered with IANA" ;
    ns1:definition " Identifies service documentation for the context that is primarily intended for human consumption."@en ;
    ns1:notation "service-doc"^^<https://www.iana.org/assignments/link-relations> ;
    ns1:prefLabel "service-doc"@en .

<http://www.opengis.net/def/rel/ogc/1.0/conformance> a ns1:Concept ;
    rdfs:label "conformance" ;
    ns3:created "2020-06-10"^^xsd:date ;
    ns2:status <http://www.opengis.net/def/status/proposed> ;
    rdfs:comment "Not yet registered with IANA" ;
    ns1:definition " Refers to a resource that identifies the specifications that the link’s context conforms to."@en ;
    ns1:notation "conformance" ;
    ns1:prefLabel "conformance"@en .

<http://www.opengis.net/def/rel/ogc/1.0/data> a ns1:Concept ;
    rdfs:label "data" ;
    ns3:created "2020-06-10"^^xsd:date ;
    ns2:status <http://www.opengis.net/def/status/proposed> ;
    rdfs:comment "Not yet registered with IANA" ;
    ns1:definition " Indicates that the link’s context is a distribution of a dataset that is an API and refers to the root resource of the dataset in the API."@en ;
    ns1:notation "data" ;
    ns1:prefLabel "data"@en .

<http://www.opengis.net/def/rel/ogc/1.0/data-meta> a ns1:Concept ;
    rdfs:label "data-meta" ;
    ns3:created "2021-04-28"^^xsd:date ;
    ns2:status <http://www.opengis.net/def/status/proposed> ;
    rdfs:comment "Not yet registered with IANA" ;
    ns1:definition "Identifies general metadata for the context (dataset or collection) that is primarily intended for consumption by machines."@en ;
    ns1:notation "data-meta" ;
    ns1:prefLabel "data-meta"@en .

<http://www.opengis.net/def/rel/ogc/1.0/items> a ns1:Concept ;
    rdfs:label "items" ;
    ns3:created "2020-06-10"^^xsd:date ;
    ns2:status <http://www.opengis.net/def/status/proposed> ;
    rdfs:comment "Not yet registered with IANA" ;
    ns1:definition " Refers to a resource that is comprised of members of the collection represented by the link’s context."@en ;
    ns1:notation "items" ;
    ns1:prefLabel "items"@en .

<http://www.opengis.net/def/rel/> a ns1:Collection ;
    rdfs:label "OGC Link Relation Type Register" ;
    ns1:definition "This is a register of link relation types used by OGC standards."@en ;
    ns1:inScheme <http://www.opengis.net/def/rel> ;
    ns1:member <http://www.opengis.net/def/rel/iana/1.0/alternate>,
        <http://www.opengis.net/def/rel/iana/1.0/collection>,
        <http://www.opengis.net/def/rel/iana/1.0/describedby>,
        <http://www.opengis.net/def/rel/iana/1.0/item>,
        <http://www.opengis.net/def/rel/iana/1.0/license>,
        <http://www.opengis.net/def/rel/iana/1.0/next>,
        <http://www.opengis.net/def/rel/iana/1.0/prev>,
        <http://www.opengis.net/def/rel/iana/1.0/self>,
        <http://www.opengis.net/def/rel/iana/1.0/service-desc>,
        <http://www.opengis.net/def/rel/iana/1.0/service-doc>,
        <http://www.opengis.net/def/rel/ogc/1.0/conformance>,
        <http://www.opengis.net/def/rel/ogc/1.0/data>,
        <http://www.opengis.net/def/rel/ogc/1.0/data-meta>,
        <http://www.opengis.net/def/rel/ogc/1.0/items> ;
    ns1:prefLabel "OGC Link Relation Type Register"@en .

