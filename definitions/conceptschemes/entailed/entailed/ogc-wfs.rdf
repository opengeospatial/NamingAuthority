    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2012-02-14</dct:modified>
    <policy:status rdf:resource="http://www.opengis.net/def/status/valid"/>
    <rdfs:seeAlso rdf:resource="http://www.opengis.net/doc/IS/WFS/2.0"/>
    <rdfs:seeAlso rdf:resource="http://www.opengis.net/doc/wfs/2.0"/>
    <owl:sameAs rdf:resource="http://www.opengis.net/def/ogc-wfs/GetFeatureById"/>
    <owl:sameAs rdf:resource="http://www.opengis.net/def/query/ogc-wfs/0/get-feature-by-id"/>
    <owl:sameAs rdf:resource="urn:ogc:def:query:OGC-WFS::GetFeatureById"/>
    <skos:altLabel xml:lang="en">GetFeatureById</skos:altLabel>
    <skos:definition xml:lang="en">All servers shall implement the GetFeature operation with the ability to execute a stored query that accepts a single argument, named "id" and of type xsd:string, and shall return a single feature with the identifier specified as it argument. "WFS 2.0, cl. 11.2.4"</skos:definition>
    <skos:inScheme rdf:resource="http://www.opengis.net/def/scheme"/>
    <skos:prefLabel xml:lang="en">Get Feature by Id</skos:prefLabel>
  </rdf:Description>
