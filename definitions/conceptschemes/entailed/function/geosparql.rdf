  <rdf:Description rdf:about="http://www.opengis.net/def/function/geosparql/envelope">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="http://www.opengis.net/spec/geosparql/1.0"/>
    <rdfs:label xml:lang="en">envelope</rdfs:label>
    <rdfs:seeAlso rdf:resource="http://www.opengis.net/doc/geosparql/1.0"/>
