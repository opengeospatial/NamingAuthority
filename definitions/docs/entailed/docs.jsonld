      {
        "@type": "http://www.opengis.net/def/metamodel/ogc-na/doc_no",
        "@value": "17-059"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#prefLabel": [
      {
        "@language": "en",
        "@value": "Technical report from the DGIWG Portrayal Technical Panel testing of SLD (1.1.0) for OGC"
      }
    ]
  },
