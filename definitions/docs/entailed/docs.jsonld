    "http://www.w3.org/2004/02/skos/core#altLabel": [
      {
        "@language": "en",
        "@value": "OGC IOGP/IPIECA Recommended Practice for a Common Operating Picture for Oil Spill Response"
      },
      {
        "@language": "en",
        "@value": "15-037"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#broader": [
