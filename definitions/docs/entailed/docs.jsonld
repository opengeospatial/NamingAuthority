    "http://www.w3.org/2004/02/skos/core#altLabel": [
      {
        "@language": "en",
        "@value": "OGC Testbed-19 — Non-Terrestrial Geospatial Engineering Report"
      },
      {
        "@language": "en",
        "@value": "23-042"
