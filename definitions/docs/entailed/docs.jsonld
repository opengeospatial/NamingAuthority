    "http://www.w3.org/2004/02/skos/core#altLabel": [
      {
        "@language": "en",
        "@value": "OGC Indexed 3d Scene Layer (I3S) and Scene Layer Package (*.slpk) Format Community Standard Version 1.3"
      },
