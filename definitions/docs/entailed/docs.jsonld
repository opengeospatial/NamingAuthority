    "http://www.w3.org/2004/02/skos/core#altLabel": [
      {
        "@language": "en",
        "@value": "OGC Federated Marine Spatial Data Infrastructure Pilot 2023 - Connecting Land and Sea for Global Awareness"
      },
      {
        "@language": "en",
        "@value": "23-027"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#broader": [
