  <rdf:Description rdf:about="http://www.opengis.net/def/docs/03-029">
    <dct:creator>Stephane Fellah, Steven Keens</dct:creator>
    <skos:broader rdf:resource="http://www.opengis.net/def/doc-type/dp"/>
