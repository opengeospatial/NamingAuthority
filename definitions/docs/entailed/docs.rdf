  <rdf:Description rdf:about="http://www.opengis.net/def/docs/08-091r6">
    <skos:altLabel xml:lang="en">Corrigendum for OpenGIS Implementation Standard Web Processing Service (WPS) 1.0.0</skos:altLabel>
    <skos:altLabel xml:lang="en">08-091r6</skos:altLabel>
    <na:doctype rdf:resource="http://www.opengis.net/def/doc-type/isc"/>
    <skos:inScheme rdf:resource="http://www.opengis.net/def/docs"/>
