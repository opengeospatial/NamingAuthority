  <rdf:Description rdf:about="http://www.opengis.net/def/docs/11-092r2">
    <skos:prefLabel xml:lang="en">OWS-8 Report on Digital NOTAM Event Specification</skos:prefLabel>
    <skos:altLabel xml:lang="en">OWS-8 Report on Digital NOTAM Event Specification</skos:altLabel>
