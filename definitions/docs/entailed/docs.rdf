  <rdf:Description rdf:about="http://www.opengis.net/def/docs/18-045">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:created rdf:datatype="xsd:date">2019-03-07</dct:created>
    <dct:creator>Jeff Harrison, Panagiotis (Peter) A. Vretanos</dct:creator>
    <rdfs:seeAlso rdf:resource="https://docs.ogc.org/per/18-045.html"/>
    <na:doctype rdf:resource="http://www.opengis.net/def/doc-type/per"/>
