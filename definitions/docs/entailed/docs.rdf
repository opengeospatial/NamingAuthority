  <rdf:Description rdf:about="http://www.opengis.net/def/docs/17-083r4">
    <dct:creator>Joan Masó, Jérôme Jacovella-St-Louis</dct:creator>
    <skos:notation rdf:datatype="http://www.opengis.net/def/metamodel/ogc-na/doc_no">17-083r4</skos:notation>
