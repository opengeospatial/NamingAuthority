    <skos:altLabel xml:lang="en">16-011r5</skos:altLabel>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:created rdf:datatype="xsd:date">2021-02-26</dct:created>
    <skos:broader rdf:resource="http://www.opengis.net/def/doc-type/bp"/>
