  <rdf:Description rdf:about="http://www.opengis.net/def/docs/17-041">
    <dct:created rdf:datatype="xsd:date">2018-02-22</dct:created>
    <na:doctype rdf:resource="http://www.opengis.net/def/doc-type/per"/>
    <skos:inScheme rdf:resource="http://www.opengis.net/def/docs"/>
