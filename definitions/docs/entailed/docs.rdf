  </rdf:Description>
  <rdf:Description rdf:about="http://www.opengis.net/def/docs/15-028">
    <dct:created rdf:datatype="xsd:date">2015-08-19</dct:created>
    <dct:creator>Daniel Balog</dct:creator>
    <na:status rdf:resource="http://www.opengis.net/def/status/valid"/>
