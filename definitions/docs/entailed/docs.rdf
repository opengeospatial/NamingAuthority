  <rdf:Description rdf:about="http://www.opengis.net/def/docs/21-013">
    <dct:creator>Robert Thomas, Josh Lieberman</dct:creator>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
