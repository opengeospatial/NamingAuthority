  <rdf:Description rdf:about="http://www.opengis.net/def/docs/13-015">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">OGC Best Practice for Sensor Web Enablement: Provision of Observations through an OGC Sensor Observation Service (SOS)</skos:prefLabel>
    <skos:inScheme rdf:resource="http://www.opengis.net/def/docs"/>
    <skos:altLabel xml:lang="en">Provision of Observations through an OGC Sensor Observation Service (SOS)</skos:altLabel>
