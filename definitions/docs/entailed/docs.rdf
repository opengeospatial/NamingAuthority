    <skos:altLabel xml:lang="en">Testbed-12 PubSub / Catalog Engineering Report</skos:altLabel>
    <skos:altLabel xml:lang="en">16-137r2</skos:altLabel>
    <rdfs:seeAlso rdf:resource="https://docs.ogc.org/per/16-137r2.html"/>
    <dct:created rdf:datatype="xsd:date">2017-05-12</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
