    <skos:altLabel xml:lang="en">Best Practice for Sensor Web Enablement Lightweight SOS Profile for Stationary In-Situ Sensors</skos:altLabel>
    <skos:altLabel xml:lang="en">11-169r1</skos:altLabel>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
