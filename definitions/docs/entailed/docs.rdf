    <skos:altLabel xml:lang="en">14-003</skos:altLabel>
    <skos:altLabel xml:lang="en">WaterML-WQ – an O&amp;M and WaterML 2.0 profile for water quality data</skos:altLabel>
    <dct:created rdf:datatype="xsd:date">2014-12-02</dct:created>
    <skos:prefLabel xml:lang="en">WaterML-WQ – an O&amp;M and WaterML 2.0 profile for water quality data</skos:prefLabel>
    <skos:broader rdf:resource="http://www.opengis.net/def/doc-type/bp"/>
