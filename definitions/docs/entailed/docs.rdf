    <skos:inScheme rdf:resource="http://www.opengis.net/def/docs"/>
    <dct:created rdf:datatype="xsd:date">2019-10-23</dct:created>
    <dct:creator>Craig A. Lee</dct:creator>
  </rdf:Description>
