  <rdf:Description rdf:about="http://www.opengis.net/def/docs/16-115">
    <dct:creator>Guy Schumann</dct:creator>
    <skos:altLabel xml:lang="en">Future City Pilot 1 - Recommendations on Serving IFC via WFS</skos:altLabel>
    <skos:altLabel xml:lang="en">16-115</skos:altLabel>
    <rdfs:seeAlso rdf:resource="https://docs.ogc.org/per/16-115.html"/>
