    <skos:broader rdf:resource="http://www.opengis.net/def/doc-type/bp"/>
    <skos:prefLabel xml:lang="en">Defence Geospatial Information Working Group (DGIWG) GMLJP2/JP2 Profile for Imagery &amp; Gridded Data 2.1.2</skos:prefLabel>
    <skos:inScheme rdf:resource="http://www.opengis.net/def/docs"/>
    <na:status rdf:resource="http://www.opengis.net/def/status/valid"/>
    <skos:altLabel xml:lang="en">21-007</skos:altLabel>
    <skos:altLabel xml:lang="en">Defence Geospatial Information Working Group (DGIWG) GMLJP2/JP2 Profile for Imagery &amp; Gridded Data 2.1.2</skos:altLabel>
    <na:doctype rdf:resource="http://www.opengis.net/def/doc-type/bp"/>
    <dct:creator>DGIWG</dct:creator>
    <rdfs:seeAlso rdf:resource="https://docs.ogc.org/bp/21-007/21-007.pdf"/>
    <skos:definition>This document provides a profile for JPEG 2000 for use as a&#13;
compression format for raster imagery. JPEG 2000 uses&#13;
discrete wavelet transform (DWT) for compressing raster data,&#13;
as opposed to the JPEG standard, which uses discrete cosine&#13;
transform (DCT). It is a compression technology which is best&#13;
suited for continuous raster data, such as satellite imagery and&#13;
aerial photography. This version adds support for&#13;
Referenceable imagery.</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.opengis.net/def/docs/12-103r3">
