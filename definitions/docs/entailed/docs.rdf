  <rdf:Description rdf:about="http://www.opengis.net/def/docs/02-059">
    <skos:altLabel xml:lang="en">Filter Encoding</skos:altLabel>
    <skos:altLabel xml:lang="en">02-059</skos:altLabel>
    <dct:created rdf:datatype="xsd:date">2001-05-01</dct:created>
    <skos:prefLabel xml:lang="en">Filter Encoding</skos:prefLabel>
    <dct:creator>Peter Vretanos</dct:creator>
    <rdfs:seeAlso rdf:resource="https://portal.ogc.org/files/?artifact_id=1171"/>
    <skos:definition>A filter is a construct used to describe constraints on properties of a feature class for the purpose of identifying a subset of feature instances to be operated upon in some way.</skos:definition>
