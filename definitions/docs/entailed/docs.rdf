    <na:status rdf:resource="http://www.opengis.net/def/status/valid"/>
    <rdfs:seeAlso rdf:resource="https://portal.ogc.org/files/?artifact_id=64644"/>
    <skos:notation rdf:datatype="http://www.opengis.net/def/metamodel/ogc-na/doc_no">15-075r1</skos:notation>
    <skos:broader rdf:resource="http://www.opengis.net/def/doc-type/dp"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.opengis.net/def/docs/11-073r2">
    <dct:creator>Debbie Wilson, Ian Painter </dct:creator>
    <skos:altLabel xml:lang="en">11-073r2</skos:altLabel>
    <skos:altLabel xml:lang="en">OWS-8 Aviation: Guidance for Retrieving AIXM 5.1 data via an OGC WFS 2.0</skos:altLabel>
    <skos:definition>The scope of this guidelines report is to provide:&#13;
1.	Overview of the OGC WFS 2.0 standard&#13;
2.	Recommendations for a minimum set of operations and behaviours that should be supported to ensure consistency across software implementations.  &#13;
3.	Guidance for configuring the WFS 2.0 to retrieve AIXM 5.1 data &#13;
4.	Summary of potential improvements to WFS/FE 2.0, GML and AIXM 5.1 specifications to better support aeronautical use cases&#13;
</skos:definition>
