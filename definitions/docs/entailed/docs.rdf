    <skos:altLabel xml:lang="en">20-067</skos:altLabel>
    <skos:altLabel xml:lang="en">Second Environmental Linked Features Experiment</skos:altLabel>
    <dct:creator>David Blodgett</dct:creator>
    <skos:inScheme rdf:resource="http://www.opengis.net/def/docs"/>
    <dct:created rdf:datatype="xsd:date">2020-10-22</dct:created>
  </rdf:Description>
