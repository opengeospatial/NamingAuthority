    <skos:altLabel xml:lang="en">Testbed-11 Test and Demonstration Results for NIEM using IC Data Encoding Specifications Engineering Report</skos:altLabel>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <na:doctype rdf:resource="http://www.opengis.net/def/doc-type/per"/>
    <skos:inScheme rdf:resource="http://www.opengis.net/def/docs"/>
