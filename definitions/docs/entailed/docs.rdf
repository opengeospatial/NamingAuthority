  <rdf:Description rdf:about="http://www.opengis.net/def/docs/17-059">
    <skos:altLabel xml:lang="en">17-059</skos:altLabel>
    <skos:altLabel xml:lang="en">Technical report from the DGIWG Portrayal Technical Panel testing of SLD (1.1.0) for OGC</skos:altLabel>
    <na:status rdf:resource="http://www.opengis.net/def/status/valid"/>
