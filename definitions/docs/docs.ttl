<http://www.opengis.net/def/docs/15-005r1> a skos:Concept ;
    dct:created "2016-02-01"^^<xsd:date> ;
    dct:creator "Stefan Strobel, Dimitri Sarafinof, David Wesloh, Paul Lacey" ;
    na:doctype doctype:bp ;
