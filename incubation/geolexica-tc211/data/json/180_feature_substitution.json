{
  "term": "feature substitution",
  "termid": "180",
  "eng": {
    "dates": [
      {
        "date": "2002-09-01 00:00:00 +0600",
        "type": "accepted"
      }
    ],
    "definition": [
      {
        "content": "feature succession in which one feature instance is replaced by another feature instance of the same or different feature type"
      }
    ],
    "examples": [
      {
        "content": "An instance of feature type \"building\" is razed and replaced by an instance of feature type \"parking lot.\""
      }
    ],
    "id": "180",
    "notes": [

    ],
    "release": 1.0,
    "sources": [
