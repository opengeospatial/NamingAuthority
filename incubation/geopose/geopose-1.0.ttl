        skos:Concept ;
    skos:broader geopose:classes ;
    skos:definition "A pair of frame transform objects, used to express relationships in sequences or graphs."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:prefLabel "FrameTransformPair"@en .

geopose:GeoPose a owl:Class,
        skos:Concept ;
    rdfs:subClassOf geopose:Pose ;
    skos:broader geopose:Pose ;
    skos:definition "A top-level structural data unit combining a FrameTransform and optional time to express a pose in space and time."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:prefLabel "GeoPose"@en .

