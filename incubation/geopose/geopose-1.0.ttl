geopose:InnerFrame a owl:Class,
        skos:Concept ;
    rdfs:subClassOf geopose:Frame ;
    skos:broader geopose:Frame ;
    skos:definition "The target frame in a FrameTransform: the ‘child’ or inner coordinate system after applying the transform."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:prefLabel "InnerFrame"@en .

