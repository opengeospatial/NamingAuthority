geopose:Basic_Quaternion a owl:Class,
        skos:Concept ;
    skos:broader geopose:classes ;
    skos:definition "Basic GeoPose target using a fixed outer frame (EPSG:4979) and representing orientation via a unit quaternion."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:prefLabel "Basic_Quaternion"@en .

geopose:Basic_YPR a owl:Class,
        skos:Concept ;
    skos:broader geopose:classes ;
    skos:definition "Basic GeoPose target using a fixed outer frame (EPSG:4979) and representing orientation via yaw, pitch, roll rotations about Z, Y, X axes."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:prefLabel "Basic_YPR"@en .

geopose:FrameAndTime a owl:Class,
        skos:Concept ;
    skos:broader geopose:classes ;
    skos:definition "A structure coupling a frame (i.e. FrameSpec) with a time instant."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:prefLabel "FrameAndTime"@en .

geopose:GenericFrameSpec a owl:Class,
        skos:Concept ;
    rdfs:subClassOf geopose:ExplicitFrameSpec ;
    skos:broader geopose:ExplicitFrameSpec ;
    skos:definition "A generic frame specification class that can express arbitrary frame conventions, parameterized externally."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:narrower geopose:Quaternion_OrientedGenericSpec ;
    skos:prefLabel "GenericFrameSpec"@en .

geopose:Transition_Model a owl:Class,
        skos:Concept ;
    skos:broader geopose:classes ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:prefLabel "Transition Model"@en .

geopose:UNIXTimeMilliseconds a owl:Class,
        skos:Concept ;
    skos:broader geopose:classes ;
    skos:definition "A temporal primitive representing a time instant in milliseconds since the Unix epoch."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:prefLabel "UNIXTimeMilliseconds"@en .

geopose:YawPitchRollAngles a owl:Class,
        skos:Concept ;
    skos:broader geopose:classes ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:prefLabel "YawPitchRollAngles"@en .

geopose:Advanced a owl:Class,
        skos:Concept ;
    skos:broader geopose:classes ;
    skos:definition "A GeoPose target allowing full configuration of the inner frame via a quaternion-based orientation; more flexible than the basic targets."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:prefLabel "Advanced"@en .

geopose:Chain a owl:Class,
        skos:Concept ;
    skos:broader geopose:classes ;
    skos:definition "A composite GeoPose structure representing a linear linked sequence of frame transforms."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:prefLabel "Chain"@en .

geopose:FrameTransformIndexPair a owl:Class,
        skos:Concept ;
    skos:broader geopose:classes ;
    skos:definition "A pairing of a FrameTransform with an integer index, used in Graph structures."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:prefLabel "FrameTransformIndexPair"@en .

geopose:Graph a owl:Class,
        skos:Concept ;
    skos:broader geopose:classes ;
    skos:definition "A composite GeoPose structure representing a directed graph of frame transforms between frames."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:prefLabel "Graph"@en .

geopose:Instant a owl:Class,
        skos:Concept ;
    rdfs:subClassOf geopose:Temporal_Entity ;
    skos:broader geopose:Temporal_Entity ;
    skos:definition "A temporal primitive denoting a single point in time."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:narrower geopose:GeoPose_Instant ;
    skos:prefLabel "Instant"@en .

geopose:LTP_ENUSpec a owl:Class,
        skos:Concept ;
    rdfs:subClassOf geopose:ExplicitFrameSpec ;
    skos:broader geopose:ExplicitFrameSpec ;
    skos:definition "Local Tangent Plane (East, North, Up) frame specification, often used as an inner frame reference."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:narrower geopose:Oriented_LTP_ENUSpec ;
    skos:prefLabel "LTP_ENUSpec"@en .

geopose:Proper_Interval a owl:Class,
        skos:Concept ;
    rdfs:subClassOf geopose:Temporal_Entity ;
    skos:broader geopose:Temporal_Entity ;
    skos:definition "A time interval with definite start and end instants (non-degenerate)."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:prefLabel "Proper_Interval"@en .

geopose:SeriesTrailer a owl:Class,
        skos:Concept ;
    skos:broader geopose:classes ;
    skos:definition "Trailer metadata for a time-series GeoPose (series), typically including integrity checks and count."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:prefLabel "SeriesTrailer"@en .

geopose:Stream a owl:Class,
        skos:Concept ;
    rdfs:subClassOf geopose:Sequence ;
    skos:broader geopose:Sequence ;
    skos:definition "A possibly open-ended sequence of time-stamped GeoPose elements streamed in time order."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:prefLabel "Stream"@en .

geopose:StreamHeader a owl:Class,
        skos:Concept ;
    skos:broader geopose:classes ;
    skos:definition "Header metadata for a streaming GeoPose sequence, including transition model and outer frame."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:prefLabel "StreamHeader"@en .

geopose:Temporal_Reference_Frame a owl:Class,
        skos:Concept ;
    skos:broader geopose:classes ;
    skos:definition "A reference frame or convention for interpreting temporal positions (time axis)."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:prefLabel "Temporal_Reference_Frame"@en .

geopose:AbstractFrameSpec a owl:Class,
        skos:Concept ;
    skos:broader geopose:classes ;
    skos:definition "An abstract base class for frame specifications defining properties common to all frame specifiers."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:narrower geopose:EPSG4979,
        geopose:ExplicitFrameSpec ;
    skos:prefLabel "AbstractFrameSpec"@en .

geopose:FrameAndTimeElement a owl:Class,
        skos:Concept ;
    skos:broader geopose:classes ;
    skos:definition "An element used in sequences/streams combining an explicit frame spec with a GeoPose instant."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:prefLabel "FrameAndTimeElement"@en .

geopose:GeoPose_Duration a owl:Class,
        skos:Concept ;
    skos:broader geopose:classes ;
    skos:definition "Represents a duration of time, used in temporal modeling of GeoPoses."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:prefLabel "GeoPose_Duration"@en .

geopose:Pose a owl:Class,
        skos:Concept ;
    skos:broader geopose:classes ;
    skos:definition "A conceptual object representing both translation and orientation between an outer and inner frame."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:narrower geopose:GeoPose ;
    skos:prefLabel "Pose"@en .

geopose:TransitionModel a owl:Class,
        skos:Concept ;
    skos:broader geopose:classes ;
    skos:definition "An enumeration defining how to interpolate between poses (e.g. 'interpolate')."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:prefLabel "TransitionModel"@en .

geopose:GeoPose_Instant a owl:Class,
        skos:Concept ;
    rdfs:subClassOf geopose:Instant ;
    skos:broader geopose:Instant ;
    skos:definition "Combines a GeoPose (frame transform) with a time instant to represent position+orientation at one time."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:prefLabel "GeoPose_Instant"@en .

geopose:Irregular_Series a owl:Class,
        skos:Concept ;
    rdfs:subClassOf geopose:Series ;
    skos:broader geopose:Series ;
    skos:definition "A time-series GeoPose where the time stamps may vary per record (nonuniform spacing)."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:prefLabel "Irregular Series"@en,
        "Irregular_Series"@en .

geopose:Oriented_LTP_ENUSpec a owl:Class,
        skos:Concept ;
    rdfs:subClassOf geopose:LTP_ENUSpec ;
    skos:broader geopose:LTP_ENUSpec ;
    skos:definition "A variant of LTP-ENU spec with an explicit orientation (beyond just defining the tangent plane)."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:narrower geopose:Quaternion_OrientedLTP_ENUSpec,
        geopose:YPR_OrientedLTP_ENUSpec ;
    skos:prefLabel "Oriented_LTP_ENUSpec"@en .

geopose:Temporal_Entity a owl:Class,
        skos:Concept ;
    skos:broader geopose:classes ;
    skos:definition "A superclass for temporal objects such as instants, intervals, positions."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:narrower geopose:Instant,
        geopose:Proper_Interval ;
    skos:prefLabel "Temporal_Entity"@en .

geopose:Temporal_Position a owl:Class,
        skos:Concept ;
    skos:broader geopose:classes ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:narrower geopose:Date_Time,
        geopose:Time_Position ;
    skos:prefLabel "Temporal_Position"@en .

geopose:Time_Position a owl:Class,
        skos:Concept ;
    rdfs:subClassOf geopose:Temporal_Position ;
    skos:broader geopose:Temporal_Position ;
    skos:definition "A generic temporal class for representing a position in time (e.g. instant or interval endpoint)."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:prefLabel "Time_Position"@en .

geopose:Sequence a owl:Class,
        skos:Concept ;
    skos:broader geopose:classes ;
    skos:definition "An abstract superclass for ordered collections of GeoPose elements (Series, Stream, etc.)."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:narrower geopose:Series,
        geopose:Stream ;
    skos:prefLabel "Sequence"@en .

geopose:SeriesHeader a owl:Class,
        skos:Concept ;
    skos:broader geopose:classes ;
    skos:definition "Header metadata for a time-series GeoPose (series), including outer frame and transition model."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:prefLabel "SeriesHeader"@en .

geopose:Unit_Quaternion a owl:Class,
        skos:Concept ;
    skos:broader geopose:classes ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:prefLabel "Unit Quaternion"@en .

geopose:Frame a owl:Class,
        skos:Concept ;
    skos:broader geopose:classes ;
    skos:definition "Represents a reference frame in the GeoPose model, identified by a FrameSpec."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:narrower geopose:InnerFrame,
        geopose:OuterFrame ;
    skos:prefLabel "Frame"@en .

geopose:Regular_Series a owl:Class,
        skos:Concept ;
    rdfs:subClassOf geopose:Series ;
    skos:broader geopose:Series ;
    skos:definition "A time-series GeoPose with fixed, uniform time spacing between successive poses."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:prefLabel "Regular Series"@en,
        "Regular_Series"@en .

geopose:Series a owl:Class,
        skos:Concept ;
    rdfs:subClassOf geopose:Sequence ;
    skos:broader geopose:Sequence ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:narrower geopose:Irregular_Series,
        geopose:Regular_Series ;
    skos:prefLabel "Series"@en .

geopose:ExplicitFrameSpec a owl:Class,
        skos:Concept ;
    rdfs:subClassOf geopose:AbstractFrameSpec ;
    skos:broader geopose:AbstractFrameSpec ;
    skos:definition "A frame specification defined explicitly (by authority, id, parameters) and used for linking or referencing frames."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:narrower geopose:GenericFrameSpec,
        geopose:LTP_ENUSpec,
        geopose:LTP_NEDSpec,
        geopose:Look_AtSpecification,
        geopose:MatrixSpec ;
    skos:prefLabel "ExplicitFrameSpec"@en .

