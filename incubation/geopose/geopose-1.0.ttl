    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:prefLabel "AtAndUpVectors"@en .

geopose:Date_Time a owl:Class,
        skos:Concept ;
    rdfs:subClassOf geopose:Temporal_Position ;
    skos:broader geopose:Temporal_Position ;
    skos:definition "A text or encoding form for representing calendar-based date/time, external to core GeoPose time model."@en ;
    skos:inScheme <https://w3id.org/ogc/geopose> ;
    skos:prefLabel "Date_Time"@en .

geopose:EPSG4979 a owl:Class,
        skos:Concept ;
    rdfs:subClassOf geopose:AbstractFrameSpec ;
    skos:broader geopose:AbstractFrameSpec ;
